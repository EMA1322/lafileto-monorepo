{
  "name": "backend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo \"No build step for pure JS backend\"",
    "postinstall": "prisma generate",
    "lint": "pnpm exec eslint . --no-error-on-unmatched-pattern",
    "test": "node ./scripts/test.mjs",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset --force --skip-seed",
    "lint:fix": "pnpm exec eslint . --fix --no-error-on-unmatched-pattern",
    "format": "pnpm exec prettier . --write",
    "db:seed": "node prisma/seed.js",
    "seed": "pnpm run db:seed",
    "db:reset": "pnpm run prisma:migrate:reset && pnpm run db:seed"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.17.0",
    "supertest": "6.3.4"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "bcryptjs": "^2.4.3",
    "zod": "^4.1.12"
  }
}
